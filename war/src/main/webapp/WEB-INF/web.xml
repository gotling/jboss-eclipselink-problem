<?xml version="1.0"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
                         "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
	<display-name>Simple Web Application</display-name>
	<servlet>
		<servlet-name>index</servlet-name>
		<servlet-class>se.magos.servlet.Index</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>injection</servlet-name>
		<servlet-class>se.magos.servlet.Injection</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>transaction</servlet-name>
		<servlet-class>se.magos.servlet.Transaction</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>index</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>injection</servlet-name>
		<url-pattern>/injection</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>transaction</servlet-name>
		<url-pattern>/transaction</url-pattern>
	</servlet-mapping>
</web-app>
